@font-face {
    font-family: "Roboto";
    src: url('<%= asset_path("Roboto/Roboto-Light.eot") %>');
    src: url('<%= asset_path("Roboto/Roboto-Ligh.eot") + "?#iefix" %>') format('eot'),
    url('<%= asset_path("Roboto/Roboto-Light.woff") %>') format('woff'),
    url('<%= asset_path("Roboto-Ligh.ttf") %>') format('truetype'),
    url('<%= asset_path("Roboto/Roboto-Light.svg)") + "#Roboto-Ligh" %>') format('svg');
    font-weight: normal;
    font-style: normal;
}