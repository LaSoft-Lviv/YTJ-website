body {
  box-sizing: border-box;
}

@font-face {
  font-family: Roboto;
  src: url(Roboto/Roboto-Light.eot);
  src: local('Bq+@');
  src: url(Roboto/Roboto-Light.woff) format('woff');
  src: url(Roboto/Roboto-Light.svg) format('svg');
  src: url(Roboto/Roboto-Light.ttf) format('truetype');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: Roboto;
  src: url(Roboto/Roboto-Medium.ttf);
  font-weight: bold;
}

@font-face {
  font-family: Roboto;
  src: url(Roboto/Roboto-Thin.ttf);
  font-weight: 100;
}

@font-face {
  font-family: Roboto;
  src: url(Roboto/Roboto-Regular.ttf);
  font-weight: 300;
}

@font-face {
  font-family: Roboto;
  src: url(Roboto/Roboto-Bold.ttf);
  font-weight: 600;
