/*CAROUSEL*/
.container-carousel-main {
  padding: 0px;
}

.container-carousel-submain {
  padding: 0px;
}

.img-responsive-main {
    display: block;
    max-width: 100%;
    height: auto;
}

.image-shown {
  display: inline-block;
}

.image-hidden {
  display: none;
}

.carousel-inner-main {
  width: 100%;
  position: relative;
  float: left;
}

.next-link {
  position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    width: 15%;
    font-size: 20px;
    color: #fff;
    text-align: center;
    text-shadow: 0 1px 2px rgba(0,0,0,.6);
    opacity: .5;
    cursor: pointer;
    z-index: 100;
}

.previous-link {
  position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    width: 15%;
    font-size: 20px;
    color: #fff;
    text-align: center;
    text-shadow: 0 1px 2px rgba(0,0,0,.6);
    opacity: .5;
    cursor: pointer;
    z-index: 100;
}

.switch-next-slide {
  position: absolute;
    top: 50%;
    right: 50%;
    z-index: 200;
    display: inline-block;
    width: 30px;
    height: 30px;
    margin-top: -15px;
    margin-right: -15px;
    font-size: 30px;
    color: #fff;
    text-align: center;
    text-shadow: 0 1px 2px rgba(0,0,0,.6);
    cursor: pointer;
    opacity: 0;
}

.switch-previous-slide {
  position: absolute;
    top: 50%;
    left: 50%;
    z-index: 200;
    display: inline-block;
    width: 30px;
    height: 30px;
    margin-top: -15px;
    font-size: 30px;
    color: #fff;
    text-align: center;
    text-shadow: 0 1px 2px rgba(0,0,0,.6);
    cursor: pointer;
    opacity: 0;
}

.carousel-indicators-main {
    position: absolute;
    bottom: 10px;
    left: 50%;
    z-index: 200;
    width: 60%;
    padding-left: 0;
    margin-left: -30%;
    text-align: center;
    list-style: none;
}

.carousel-indicators-main li.active {
    width: 12px;
    height: 12px;
    margin: 0;
    background-color: #fff;
}

.carousel-indicators-main li {
    display: inline-block;
    width: 10px;
    height: 10px;
    margin: 1px;
    text-indent: -999px;
    background-color: rgba(0,0,0,0);
    border: 1px solid #fff;
    border-radius: 10px;
}

.carousel-description {
  display: none;
  position: absolute;
  bottom: 50px;
  left: 20%;
  width: 60%;
  font-size: 18px;
  color: #fff;
  text-align: center;
  z-index: 200;
  text-shadow: 0 1px 2px rgba(0,0,0,.6);
  opacity: 0.7;
  background-color: #000;
  list-style: none;
}

.container-description-hidden {
  display: none;
}

.container-description-shown {
  display: block;
}

.description-hidden {
  display: none;
}

.description-shown {
  display: inline;
}

.carousel-loader {
    display: block;
    position: absolute;
    left: 48%;
    top: 300px;
    z-index: 10px;
}

@media screen and (max-width: 992px) {
  .carousel-description {
    font-size: 16px;
  }
}

@media screen and (max-width: 768px) {
  .carousel-description {
    font-size: 10px;
  }
}

@media screen and (max-width: 600px) {
  .carousel-description {
    font-size: 8px;
  }
}

@media screen and (max-width: 400px) {
  .carousel-description {
    font-size: 6px;
  }
}

