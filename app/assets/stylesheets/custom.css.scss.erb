/*HEADER*/

/*Header top*/
.header-container {
  position: relative;
  height: 100px;
  padding: 0;
  margin: 0;
  background: #FDD73E;
  text-align: center;
}

.header-center {
  float: left;
  text-align: center;
  font-size: 14px;
  top: 20px;
}

.header-center a {
  display: block;
  font-family: Roboto, sans-serif;
  font-weight: bold;
  color: black;
  font-size: 18px;
  text-shadow: 4px 4px 3px black;
  text-decoration: none;
}

.header-center p {
  font-family: Myriad, sans-serif;
  font-weight: 300;
  color: black;
}

.main-logo-box {
  top: 10%;
  float: left;
  text-align: center;
  perspective: 300px;
  -webkit-perspective: 300px;
}

.main-logo {
  width: 100px;
  height: 80px;
  display: inline-block;
}

.header-right {
  text-align: center;
  top: 34px;
}

.header-right ul li {
  display: inline;
  margin-right: 10px;
}

.header-right ul li a {
  display: inline;
  font-family: Roboto, sans-serif;
  font-weight: 300;
  color: black;
  font-size: 16px;
  text-decoration: none;
}

.header-right ul li a:hover {
  cursor: pointer;
}
/*Finish header top*/

/*Nav menu*/
nav {
  height: 50px;
}

.navbar-inverse .navbar-nav>li>a {
  color: #ffffff;
}

#navbar-main {
  background-color: #202020;
  border: none;
  border-radius: 0px;
  margin-bottom: 0px;
}

.navbar-left > li >a {
  font-family: Roboto, sans-serif;
  font-weight: normal;
  color: white;
  text-transform: uppercase;
  font-size: 14px;
  letter-spacing: 4px;
}

li.donate-after-collapse, li.contact-after-collapse {
  display: none;
}

.navbar-right > li >a {
  font-family: Roboto, sans-serif;
  font-weight: normal;
  color: white;
  text-transform: uppercase;
  font-size: 14px;
  letter-spacing: 4px;
}

#nav-button {
  float: left;
  margin-left: 15px;
  color: white;
}

.collapsed {
  border: none;
}

.button-after-collapse {
  display: none;
  float: right;
  line-height: 50px;
  margin-right: 10px;
}

.button-after-collapse li a {
  color: white;
  text-decoration: none;
}
/*Finish nav menu*/

/*Admin menu*/
.admingoMain a {
  text-transform: none;
  font-size: 14px;
}

.adminName a {
  text-transform: none;
  font-size: 14px;
}

.newPageElement a span {
  font-size: 12px;
}

.newPageElementDropdown > li {
  width: 100%;
}

.newPageElementDropdown > li > a {
  color: #000000;
  height: 25px;
}

.newPageElementSettings a span {
  font-size: 12px;
}

.newPageElementSettingsDropdown > li {
  width: 100%;
}

.newPageElementSettingsDropdown > li > a {
  color: #000000;
  height: 25px;
}
/*Finish admin menu*/

@media screen and (max-width: 1200px) {
  .navbar-left > li >a {
    font-size: 12px;
  }
  .navbar-right > li >a {
    font-size: 12px;
  }
  .admingoMain a {
    font-size: 12px;
  }
  .adminName a {
    font-size: 12px;
  }
  .header-right ul li a {
    font-size: 14px;
  }
}

@media screen and (max-width: 1100px) {
  .navbar-left > li >a {
    font-size: 11px;
    letter-spacing: 2px;
  }
  .navbar-right > li >a {
    font-size: 11px;
    letter-spacing: 2px;
  }
  .admingoMain a {
    font-size: 11px;
    letter-spacing: 2px;
  }
  .adminName a {
    font-size: 11px;
    letter-spacing: 2px;
  }
  .header-right ul li a {
    font-size: 13px;
  }
}

@media screen and (max-width: 1000px) {
  .navbar-left > li >a {
    font-size: 10px;
    letter-spacing: 2px;
  }
  .navbar-right > li >a {
    font-size: 10px;
    letter-spacing: 2px;
  }
  .admingoMain a {
    font-size: 10px;
    letter-spacing: 2px;
  }
  .adminName a {
    font-size: 10px;
    letter-spacing: 2px;
  }
  .header-right ul li a {
    font-size: 12px;
  }
  .header-center a {
    font-size: 17px;
  }
  .header-center p {
    font-size: 12px;
  }
  .newPageElement a span {
    font-size: 10px;
  }
  .newPageElementSettings a span {
    font-size: 10px;
  }
}

@media screen and (max-width: 900px) {
  .navbar-left > li >a {
    font-size: 9px;
    letter-spacing: 0px;
  }
  .navbar-right > li >a {
    font-size: 9px;
    letter-spacing: 0px;
  }
  .admingoMain a {
    font-size: 9px;
    letter-spacing: 0px;
  }
  .adminName a {
    font-size: 9px;
    letter-spacing: 0px;
  }
  .header-right ul li a {
    font-size: 11px;
  }
}

@media screen and (max-width: 768px) {
  .main-logo {
    display: none;
  }
  .header-right {
    display: none;
  }
  .header-left {
    float: none;
  }
  .button-after-collapse {
    display: block;
  }
  li.donate-after-collapse, li.contact-after-collapse {
    display: block;
  }
  li.nav-after-collapse {
    display: none;
  }
}

